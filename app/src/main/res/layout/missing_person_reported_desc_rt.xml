<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contactNumber_TV"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="384sp"
            android:layout_height="571sp"
            android:layout_marginTop="32sp"
            android:layout_marginBottom="37sp"
            android:background="@drawable/imageborder"
            android:orientation="vertical"
            android:padding="16dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView5">

            <TextView
                android:id="@+id/titleDetailsReported_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Title: "
                android:textSize="18sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <TextView
                android:id="@+id/assignedTeam_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="AssignedTeam" />

            <TextView
                android:id="@+id/isFound_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="isFound" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <TextView
                android:id="@+id/missingFullName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Full Name: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/fullNameReported_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="fullName" />

            <TextView
                android:id="@+id/age"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Age: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/ageReported_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="age" />

            <TextView
                android:id="@+id/sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sex: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/sexReported_TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="sex" />

            <TextView
                android:id="@+id/areaLastSeen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Area Last Seen: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/areaLastSeenReported_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />

            <TextView
                android:id="@+id/description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Description: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/description_missing_Reported_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="description" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="25dp" />

            <TextView
                android:id="@+id/reportedBy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reported By:"
                android:textSize="18sp"
                android:textStyle="bold" />

            <Space
                android:layout_width="match_parent"
                android:layout_height="10dp" />

            <TextView
                android:id="@+id/filedBy_Reported_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="File By: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/contactNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Emergency Contact Number: "
                android:textStyle="bold"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/ContactNumberReported_TV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />

            <Button
                android:id="@+id/foundButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Found!" />
        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="409dp"
            android:layout_height="64dp"
            android:layout_marginEnd="2dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:id="@+id/Rectangle2"
                android:layout_width="412dp"
                android:layout_height="51dp"
                android:layout_marginBottom="13dp"
                android:src="@drawable/rectangle"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <ImageButton
                android:id="@+id/pna_RT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="38dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="326dp"
                android:layout_marginBottom="2dp"
                android:src="@drawable/outline_article_24"
                app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
                app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
                app:layout_constraintStart_toStartOf="@+id/Rectangle2"
                app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

            <ImageButton
                android:id="@+id/map_RT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="109dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="255dp"
                android:layout_marginBottom="2dp"
                android:src="@drawable/baseline_map_24"
                app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
                app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
                app:layout_constraintStart_toStartOf="@+id/Rectangle2"
                app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

            <ImageButton
                android:id="@+id/dashboard_RT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="329dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="35dp"
                android:layout_marginBottom="2dp"
                android:src="@drawable/baseline_align_vertical_bottom_24"
                app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
                app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="@+id/Rectangle2"
                app:layout_constraintTop_toTopOf="@+id/Rectangle2"
                app:layout_constraintVertical_bias="1.0" />

            <ImageButton
                android:id="@+id/file_RT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="254dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="110dp"
                android:layout_marginBottom="2dp"
                android:src="@drawable/baseline_report_24"
                app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
                app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
                app:layout_constraintStart_toStartOf="@+id/Rectangle2"
                app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

            <ImageButton
                android:id="@+id/help_RT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="182dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="182dp"
                android:layout_marginBottom="3dp"
                android:src="@drawable/baseline_emergency_share_24"
                app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
                app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
                app:layout_constraintStart_toStartOf="@+id/Rectangle2"
                app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="262sp"
            android:layout_height="56sp"
            android:layout_marginStart="16sp"
            android:layout_marginTop="28sp"
            android:layout_marginEnd="133sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle" />

        <TextView
            android:id="@+id/PageTitle_missingDesc_TV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="41sp"
            android:layout_marginTop="44sp"
            android:layout_marginEnd="161sp"
            android:fontFamily="sans-serif-medium"
            android:text="Missing Person Details"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>