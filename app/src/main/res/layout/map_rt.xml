<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#F0F0F0"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="410sp"
            android:layout_height="60sp"
            android:padding="1sp"
            android:paddingTop="1sp"
            android:paddingLeft="1sp"
            android:paddingBottom="1sp"
            android:paddingRight="1sp"
            android:cropToPadding="true"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/navbarbg" />

        <ImageButton
            android:id="@+id/pna_RT"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginStart="55sp"
            android:layout_marginEnd="321sp"
            android:layout_marginBottom="6sp"
            android:background="@color/white"
            android:src="@drawable/outline_article_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/map_RT"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginStart="100sp"
            android:layout_marginEnd="187sp"
            android:layout_marginBottom="6sp"
            android:background="@color/white"
            android:src="@drawable/baseline_map_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/dashboard_RT"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginStart="200sp"
            android:layout_marginEnd="110sp"
            android:layout_marginBottom="6sp"
            android:background="@color/white"
            android:src="@drawable/baseline_call_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <ImageButton
            android:id="@+id/help_RT"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_marginStart="319sp"
            android:layout_marginEnd="57sp"
            android:layout_marginBottom="6sp"
            android:background="@color/white"
            android:src="@drawable/baseline_emergency_share_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="211sp"
            android:layout_height="21sp"
            android:layout_marginStart="16sp"
            android:layout_marginTop="12sp"
            android:layout_marginEnd="184sp"
            android:paddingLeft="5sp"
            android:background="@drawable/bg2"
            android:spinnerMode="dropdown"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="90sp"
            android:layout_height="37sp"
            android:layout_marginStart="16sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="300sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.692"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="41sp"
            android:layout_marginTop="16sp"
            android:layout_marginEnd="326sp"
            android:text="Maps"
            android:fontFamily="@font/roboto_bold"
            android:textColor="#F2FFFFFF"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <FrameLayout
            android:layout_width="343sp"
            android:layout_height="546sp"
            android:layout_marginStart="25sp"
            android:layout_marginTop="119sp"
            android:layout_marginEnd="25sp"
            android:layout_marginBottom="120sp"
            android:background="@drawable/bg2"
            app:layout_constraintBottom_toBottomOf="@+id/imageView6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <fragment
                android:id="@+id/mapView"
                android:name="com.google.android.gms.maps.SupportMapFragment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>