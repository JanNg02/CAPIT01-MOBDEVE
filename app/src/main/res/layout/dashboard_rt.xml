<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#79FFFFFF"
    android:backgroundTint="#8E8D8D"
    android:foregroundTint="#79FFFFFF"
    tools:context=".dashboard.dashboardActivity_rt">

    <ImageView
        android:id="@+id/Rectangle"
        android:layout_width="410dp"
        android:layout_height="76dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="1dp"
        android:src="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/pic_RT"
        android:layout_width="63dp"
        android:layout_height="61dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/usernameRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="317dp"
        android:layout_marginBottom="29dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="409dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="667dp">

        <ImageView
            android:id="@+id/Rectangle2"
            android:layout_width="412dp"
            android:layout_height="51dp"
            android:layout_marginBottom="13dp"
            android:src="@drawable/rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/pna_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="326dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/outline_article_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/map_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="109dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="255dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_map_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/dashboard_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="329dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_align_vertical_bottom_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/file_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="254dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_report_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/help_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="182dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="182dp"
            android:layout_marginBottom="3dp"
            android:src="@drawable/baseline_emergency_share_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/logout_button_RT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/baseline_logout_24" />

    <Button
        android:id="@+id/missingReported_button_rt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:text="Missing Persons Reported"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rectangle" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="75dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/missingReported_button_rt" />

</androidx.constraintlayout.widget.ConstraintLayout>