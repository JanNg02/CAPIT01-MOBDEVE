<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#79FFFFFF"
    android:backgroundTint="#8E8D8D"
    android:foregroundTint="#79FFFFFF"
    tools:context=".dashboard.dashboardActivity_rt">

    <ImageView
        android:id="@+id/Rectangle"
        android:layout_width="410dp"
        android:layout_height="76dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="1dp"
        android:src="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/pic_RT"
        android:layout_width="63dp"
        android:layout_height="61dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/usernameRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="317dp"
        android:layout_marginBottom="29dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="409dp"
        android:layout_height="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="667dp">

        <ImageView
            android:id="@+id/Rectangle2"
            android:layout_width="412dp"
            android:layout_height="51dp"
            android:layout_marginBottom="13dp"
            android:src="@drawable/rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/pna_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="326dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/outline_article_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/map_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="109dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="255dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_map_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/dashboard_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="329dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_align_vertical_bottom_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/file_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="254dp"
            android:layout_marginTop="6dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="2dp"
            android:src="@drawable/baseline_report_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/help_RT"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="182dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="182dp"
            android:layout_marginBottom="3dp"
            android:src="@drawable/baseline_emergency_share_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/missingRT"
        android:layout_width="280dp"
        android:layout_height="36dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="58dp"
        android:layout_marginEnd="67dp"
        android:text="Missing persons reported:"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rectangle" />

    <TextView
        android:id="@+id/missingCountRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="184dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="187dp"
        android:text="123"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/missingRT" />

    <TextView
        android:id="@+id/demandRT"
        android:layout_width="280dp"
        android:layout_height="36dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="67dp"
        android:text="On-remand rescue:"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/missingCountRT" />

    <TextView
        android:id="@+id/demandCountRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="185dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="186dp"
        android:text="123"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/demandRT" />

    <TextView
        android:id="@+id/safeRT"
        android:layout_width="224dp"
        android:layout_height="63dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="92dp"
        android:text="Amount of people who are safe:"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/demandCountRT" />

    <TextView
        android:id="@+id/safeCountRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="185dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="186dp"
        android:text="123"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/safeRT" />

</androidx.constraintlayout.widget.ConstraintLayout>