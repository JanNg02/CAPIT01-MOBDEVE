<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/filebo_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#79FFFFFF"
    android:backgroundTint="#8E8D8D"
    android:foregroundTint="#79FFFFFF"
    tools:context=".file.fileActivity_bo">

    <ImageView
        android:id="@+id/Rectangle"
        android:layout_width="410dp"
        android:layout_height="76dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="1dp"
        android:src="@drawable/rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/picRT"
        android:layout_width="63dp"
        android:layout_height="61dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle"
        tools:src="@tools:sample/avatars" />

    <TextView
        android:id="@+id/usernameRT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="317dp"
        android:layout_marginBottom="29dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/Rectangle"
        app:layout_constraintEnd_toEndOf="@+id/Rectangle"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/Rectangle"
        app:layout_constraintTop_toTopOf="@+id/Rectangle" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="409dp"
        android:layout_height="64dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/Rectangle2"
            android:layout_width="412dp"
            android:layout_height="51dp"
            android:layout_marginBottom="13dp"
            android:src="@drawable/rectangle"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/response_BO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="326dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/baseline_view_list_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/rescueboard_BO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="109dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="255dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/baseline_dashboard_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/fingerprint_BO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="329dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="35dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/baseline_fingerprint_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/filearea_BO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="254dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="1dp"
            android:src="@drawable/baseline_report_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

        <ImageButton
            android:id="@+id/missing_BO"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="182dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="182dp"
            android:layout_marginBottom="3dp"
            android:src="@drawable/baseline_person_search_24"
            app:layout_constraintBottom_toBottomOf="@+id/Rectangle2"
            app:layout_constraintEnd_toEndOf="@+id/Rectangle2"
            app:layout_constraintStart_toStartOf="@+id/Rectangle2"
            app:layout_constraintTop_toTopOf="@+id/Rectangle2" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <EditText
        android:id="@+id/locationBO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="68dp"
        android:ems="10"
        android:hint="Location"
        android:inputType="textPersonName"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Rectangle" />

    <EditText
        android:id="@+id/detailsBO"
        android:layout_width="273dp"
        android:layout_height="163dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="73dp"
        android:ems="100"
        android:hint="Details of the situation"
        android:inputType="textPersonName"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/locationBO" />

    <Button
        android:id="@+id/submit_file_bo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send Report"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/detailsBO" />

</androidx.constraintlayout.widget.ConstraintLayout>